<div id="tabbed">
  <tabs>

    <tab name="active units" v-bind:selected="true">
      <%= render :partial => "unit_list", locals: {units: @active_units, state: 'active'} %>
    </tab>

    <tab name="inactive units">
      <%= render :partial => "unit_list", locals: {units: @inactive_units, state: 'inactive'} %>
    </tab>

  </tabs>
</div>

<p>&nbsp;</p>
